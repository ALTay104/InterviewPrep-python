/*Find prime number in between 10-500

bool isPrime(int n)
{
	// Corner case
	if (n <= 1)
		return false;

	// Check from 2 to n-1
	for (int i = 2; i < n; i++)
		if (n % i == 0)
			return false;

	return true;
}

//find prime number in between 10 - 500
int main() {
	// Driver Program
	for (int i = 10; i <= 500; i++)
	{
		if (isPrime(i))
		{
			cout << "Prime is " << i << endl;
		}
	}
	
	return 0;	
}
